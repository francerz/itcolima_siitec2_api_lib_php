{
    "name": "itcolima/siitec2-api-lib",
    "description": "PHP ITColima SIITEC 2 API Library",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/francerz/php-power-data"
        },
        {
            "type": "vcs",
            "url": "https://github.com/francerz/php-http"
        },
        {
            "type": "vcs",
            "url": "https://github.com/francerz/php-oauth2"
        }
    ],
    "autoload": {
        "psr-4": {
            "ITColima\\Siitec2\\Api\\" : "src"
        }
    },
    "require": {
        "psr/http-message": "^1.0",
        "francerz/php-power-data": "dev-master",
        "francerz/php-http": "dev-master",
        "francerz/php-oauth2": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    }
}